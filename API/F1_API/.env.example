STRATEGY_BOT_PYTHON=/Users/alexandruchetreanu/Documents/GitHub/F1_Manager/API/F1_API/app/Services/StrategyBot/.venv/bin/python
STRATEGY_BOT_SCRIPT=app/Services/StrategyBot/strategy_bot_openf1.py
